{
    "name": "metawesome/laravel-request-xml",
    "description": "The missing XML support for Laravel's Request class.",
    "version": "1.0.0",
    "keywords": [
        "request",
        "laravel",
        "xml",
        "isXml",
        "wantsXml",
        "api"
    ],
    "license": "MIT",
	"autoload": {
        "psr-4": {
            "Metawesome\\RequestXml\\": "src"
        }
    },
    "require": {
        "php": "~7.0",
        "illuminate/support": "^5.3",
        "illuminate/http": "^5.2",
        "metawesome/laravel-xml-to-array": "dev-master"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Metawesome\\RequestXml\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Metawesome\\RequestXml\\Providers\\RequestXmlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
